<h2>Edit Discount</h2>
<center>
  <%= form_for @discount, url: "/merchant/discounts/#{@discount.id}" do |f| %>
    <%= f.label :percentage %>
    <%= f.number_field :percentage %>

    <%= f.label :items_required, 'Items Required' %>
    <%= f.number_field :items_required %>

    <%= f.submit %>
  <% end %>
</center>
